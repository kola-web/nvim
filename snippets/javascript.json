{
  "e.currentTarget.dataset": {
    "description": "",
    "prefix": "wxt",
    "body": ["const {$1} = e.currentTarget.dataset;", ""]
  },
  "cl clg log": {
    "description": "Log output to console",
    "prefix": "cl clg log",
    "body": "console.log($1)"
