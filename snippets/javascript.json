  "vue3list": {
    "prefix": "vue3list",
    "body": [
      "<template>",
      "  <div class=\"page\">",
      "    <a-card class=\"card\">",
      "      <a-form ref=\"formRef\" :model=\"formState\" layout=\"inline\">",
      "        <a-form-item label=\"\" name=\"title\">",
      "          <a-input v-model:value=\"formState.title\" placeholder=\"请输入标题进行搜索\" />",
      "        </a-form-item>",
      "        <a-form-item label=\"创建时间\" name=\"date\">",
      "          <a-range-picker v-model:value=\"formState.date\" value-format=\"YYYY-MM-DD\"></a-range-picker>",
      "        </a-form-item>",
      "        <a-form-item :wrapper-col=\"{ span: 14, offset: 4 }\">",
      "          <a-space>",
      "            <a-button type=\"primary\" @click=\"handleSearch\">搜索</a-button>",
      "            <a-button style=\"margin-left: 10px\" @click=\"handleReset\">清除</a-button>",
      "          </a-space>",
      "        </a-form-item>",
      "      </a-form>",
      "    </a-card>",
      "    <a-card class=\"card\">",
      "      <div class=\"c-header\">",
      "        <div class=\"left\"></div>",
      "        <a-space class=\"right\">",
      "          <a-button type=\"primary\" @click=\"handleAdd\">新建</a-button>",
      "        </a-space>",
      "      </div>",
      "      <a-table",
      "        :data-source=\"dataSource\"",
      "        :columns=\"columns\"",
      "        :loading=\"loading\"",
      "        size=\"small\"",
      "        :pagination=\"pagination\"",
      "        :scroll=\"{ x: 'max-content' }\"",
      "        @change=\"handleTableChange\"",
      "      >",
      "        <template #bodyCell=\"{ column, record }\">",
      "          <a-space v-if=\"column.dataIndex === 'operation'\">",
      "            <a @click=\"handleEdit(record)\">编辑</a>",
      "            <a-popconfirm title=\"确认删除吗？\" ok-text=\"确认\" cancel-text=\"取消\" @confirm=\"handleDel(record)\">",
      "              <a>删除</a>",
      "            </a-popconfirm>",
      "          </a-space>",
      "        </template>",
      "      </a-table>",
      "    </a-card>",
      "  </div>",
      "</template>",
      "",
      "<script lang=\"ts\" setup>",
      "  import { onMounted, reactive, ref } from \"vue\";",
      "  import { useRouter } from \"vue-router\";",
      "  import { aGetRuleList, aPostDelete, aPostDisable, aPostEnable } from \"@/api/seriesOfArticles\";",
      "",
      "  const router = useRouter();",
      "  const formRef = ref(null);",
      "  const formState = reactive({",
      "    title: \"\",",
      "    date: [\"\", \"\"] as [string, string],",
      "  });",
      "  const pagination = reactive({",
      "    total: 1,",
      "    current: 1,",
      "    pageSize: 10,",
      "  });",
      "  const loading = ref(false);",
      "  const columns: any = [",
      "    {",
      "      title: \"\",",
      "      dataIndex: \"\",",
      "    },",
      "    {",
      "      title: \"操作\",",
      "      dataIndex: \"operation\",",
      "      fixed: \"right\",",
      "    },",
      "  ];",
      "  const dataSource = ref([]);",
      "",
      "  onMounted(() => {",
      "    getList();",
      "  });",
      "",
      "  function handleSearch() {",
      "    getList();",
      "  }",
      "  function handleReset() {",
      "    formRef.value?.resetFields();",
      "    getList();",
      "  }",
      "  function handleTableChange(page: any): void {",
      "    getList({ page: page.current, pageSize: page.pageSize });",
      "  }",
      "  function getList({ page, pageSize } = { page: pagination.current, pageSize: pagination.pageSize }) {",
      "    const [start_time, end_time] = formState.date;",
      "    aGetRuleList({",
      "      params: {",
      "        page,",
      "        pageSize,",
      "        ...formState,",
      "        start_time,",
      "        end_time,",
      "      },",
      "    }).then((res) => {",
      "      dataSource.value = res.datalist.data;",
      "      pagination.current = res.datalist.current_page;",
      "      pagination.total = res.datalist.total;",
      "    });",
      "  }",
      "",
      "  function handleAdd() {",
      "    router.push({",
      "      path: \"/seriesOfArticles/add\",",
      "    });",
      "  }",
      "  function handleEdit(record: any) {",
      "    router.push({",
      "      path: \"/seriesOfArticles/add\",",
      "      query: {",
      "        id: record.id,",
      "      },",
      "    });",
      "  }",
      "  function handleDel(record: any) {",
      "    aPostDelete({ id: record.id }).then(() => {",
      "      getList();",
      "    });",
      "  }",
      "</script>",
      "",
      "<style lang=\"scss\" scoped>",
      "  .page {",
      "    .card {",
      "      margin-bottom: 20px;",
      "      .c-header {",
      "        padding-bottom: 10px;",
      "        display: flex;",
      "        justify-content: space-between;",
      "      }",
      "    }",
      "  }",
      "</style>"
    ]
  },
  "wx.ajax": {
    "description": "ajax",
    "prefix": "wxa",
    "body": [
      "wx.ajax({",
      "  method: \"GET\",",
      "  url: \"\",",
      "  data: {},",
      "}).then((res) => {",
      "  console.log(\"res: \", res);",
      "});",
      ""
    ]
  }
}
